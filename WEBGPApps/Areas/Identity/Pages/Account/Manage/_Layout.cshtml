@{
    if (ViewData.TryGetValue("ParentLayout", out var parentLayout))
    {
        Layout = (string)parentLayout;
    }
    else
    {
        Layout = "/Areas/Identity/Pages/_Layout.cshtml";
    }
}
<div class="BG5">
    <div class="container ">
<h1 style="color:white; text-align:center;">Manage your account</h1>

<div>
    <h2 style="color:white; text-align:center;">Change your account settings</h2>
    <hr />
    <div class="row">
        <div class="col-md-3">
            <partial name="_ManageNav" />
        </div>
        <div class="col-md-9 wow slideInDown">
            @RenderBody()
        </div>
    </div>
</div>
</div></div>
@section Scripts {
    @RenderSection("Scripts", required: false)
}
